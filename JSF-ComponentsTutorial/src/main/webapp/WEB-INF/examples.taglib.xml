<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
		version="2.0">
	<namespace>http://it.strazz.faces/examples</namespace>
	<tag>
		<tag-name>helloWorld</tag-name>
		<component>
			<component-type>it.strazz.faces.HelloWorld</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>helloWorld2</tag-name>
		<component>
			<component-type>it.strazz.faces.HelloWorld2</component-type>
		</component>
		<attribute>
			<description><![CDATA[Attributo che indica chi salutare]]></description>
			<name>name</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<tag-name>helloWorld3</tag-name>
		<component>
			<component-type>it.strazz.faces.HelloWorld3</component-type>
		</component>
		<attribute>
			<description><![CDATA[Attributo che indica chi salutare]]></description>
			<name>name</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<tag-name>helloWorld4</tag-name>
		<component>
			<component-type>it.strazz.faces.HelloWorld4</component-type>
			<renderer-type>it.strazz.faces.HelloWorld4Renderer</renderer-type>
		</component>
		<attribute>
			<description><![CDATA[Attributo che indica chi salutare]]></description>
			<name>name</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<tag-name>clock</tag-name>
		<component>
			<component-type>it.strazz.faces.Clock</component-type>
			<renderer-type>it.strazz.faces.ClockRenderer</renderer-type>
		</component>
		<attribute>
			<description><![CDATA[true per orologio digitale, false per quello analogico]]></description>
			<name>digital</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
	</tag>
</facelet-taglib>